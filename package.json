{
  "name": "narr-material-ui",
  "version": "0.1.0",
  "description": "My UI components using Typescript+React+Material-UI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/narr/narr-material-ui.git"
  },
  "author": "https://github.com/narr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/narr/narr-material-ui/issues"
  },
  "homepage": "https://github.com/narr/narr-material-ui#readme",
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "husky": "^0.14.3",
    "lerna": "^2.11.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0"
  },
  "scripts": {
    "postinstall": "lerna bootstrap",
    "lint": "tslint && eslint",
    "precommit": "lint-staged",
    "test": "jest --config=./config/jest/jest.config.js --watch --coverage",
    "test:build": "jest --config=./config/jest/jest.config.prod.js --coverage",
    "test:ci": "npm run test:build",
    "storybook": "start-storybook -p 9001 -c ./config/.storybook",
    "storybook:build": "build-storybook -c ./config/.storybook -o ./docs/storybook",
    "build": "npm run lint && npm run test:ci && npm run storybook:build"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
