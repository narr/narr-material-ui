{
  "name": "narr-material-ui",
  "version": "0.1.0",
  "description": "My UI components using Typescript+React+Material-UI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/narr/narr-material-ui.git"
  },
  "author": "https://github.com/narr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/narr/narr-material-ui/issues"
  },
  "homepage": "https://github.com/narr/narr-material-ui/blob/master/README.md",
  "devDependencies": {
    "@material-ui/core": "^1.3.1",
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.4",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.3.0",
    "jest-environment-enzyme": "^6.0.2",
    "jest-enzyme": "^6.0.2",
    "jest-html-reporter": "^2.4.1",
    "lerna": "^2.11.0",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "styled-components": "^3.3.3",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
  "optionalDependencies": {
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "precommit": "lint-staged",
    "tslint": "tslint -c ./tslint.json -p ./",
    "eslint": "eslint **/*.js",
    "lint": "npm run tslint && npm run eslint",
    "test": "jest --config=./config/jest/jest.config.js --coverage",
    "test:watch": "npm test -- --watch",
    "test:ci": "npm run test",
    "test:build:nmui-core": "cross-env JEST_TARGET=core jest --config=./config/jest/jest.config.prod.js --coverage",
    "ci": "npm run lint && npm run test:ci",
    "storybook": "start-storybook -p 9001 -c ./config/.storybook",
    "storybook:build:nmui-core": "cross-env STORYBOOK_TARGET=core build-storybook -c ./config/.storybook -o ./docs/narr-material-ui-core/storybook",
    "build:nmui-core": "rimraf ./docs/narr-material-ui-core && npm run test:build:nmui-core && npm run storybook:build:nmui-core",
    "build:all": "npm run lint && npm run build:nmui-core",
    "publish:nmui-core": "rimraf ./packages/narr-material-ui-core/dist && tsc -p ./config/typescript/tsconfig.prod.nmui-core.json",
    "publish:all": "npm run lint && npm run publish:nmui-core"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
