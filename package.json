{
  "name": "narr-material-ui",
  "version": "0.1.0",
  "description": "My UI components using Typescript+React+Material-UI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/narr/narr-material-ui.git"
  },
  "author": "https://github.com/narr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/narr/narr-material-ui/issues"
  },
  "homepage": "https://github.com/narr/narr-material-ui#readme",
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "husky": "^0.14.3",
    "lerna": "^2.11.0",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.10",
    "prettier": "^1.13.7",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0"
  },
  "scripts": {
    "postinstall": "lerna bootstrap",
    "precommit": "lint-staged",
    "tslint": "tslint -c ./tslint.json -p ./",
    "eslint": "eslint **/*.js",
    "lint": "npm run tslint && npm run eslint",
    "test": "jest --config=./config/jest/jest.config.js --coverage",
    "test:watch": "npm test -- --watch",
    "test:ci": "npm run test",
    "test:build:nmui-core": "jest --config=./config/jest/jest.config.prod.nmui-core.js --coverage",
    "ci": "npm run lint && npm run test:ci",
    "storybook": "start-storybook -p 9001 -c ./config/.storybook",
    "storybook:build:nmui-core": "build-storybook -c ./packages/narr-material-ui-core/config/.storybook -o ./docs/narr-material-ui-core/storybook",
    "build:nmui-core": "npm run test:build:nmui-core && npm run storybook:build:nmui-core",
    "build:all": "npm run build:nmui-core"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
